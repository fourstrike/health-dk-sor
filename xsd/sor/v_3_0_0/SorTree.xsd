    <xs:sequence>
      <xs:element ref="sor1:CreatedAtTime" />
      <xs:element ref="sor1:SnapshotDate" />
      <xs:element ref="InstitutionOwnerEntity" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>
